#include<stdio.h>
int main()
{
	int n,m;
	scanf("%d %d",&n,&m);
	int a[n][m],b[n][m];
	for(int y=0;y<n;y++)
	{
		for(int x=0;x<m;x++)
		{
			scanf("%d",&a[y][x]);
			
		}
	}
	for(int y=0;y<n;y++)
	{
		for(int x=0;x<m;x++)
		{
			scanf("%d",&b[y][x]);
		}
	}
	printf("\nResult\n");
	for(int y=0;y<n;y++)
	{
		for(int x=0;x<m;x++)
		{
			int sum=0;
		     sum=a[y][x]+b[y][x];
			printf("%d ",sum);
		}
	printf("\n");	
	}

	return 0;
}   
