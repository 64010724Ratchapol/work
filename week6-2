#include<stdio.h>

int large(int a,int b,int c,int d,int e)
{
 if(a>b && a>c && a>d &&a>d)
     printf("largest is : %d",a);
 else if(b>c && b>d && b>e)
     printf("largest is : %d",b); 
 else if(c>d && c>e)
     printf("largest is : %d",c);
 else if(d>e)
     printf("largest is : %d",d);
 else
     printf("largest is : %d",e);    
}

int main()
{
	int n1,n2,n3,n4,n5,max;
	printf("Enter 5 Number : ");
	scanf("%d %d %d %d %d",&n1,&n2,&n3,&n4,&n5);
     large(n1,n2,n3,n4,n5);
     
	return 0;
}
